    { "category": "Vastaa kysymyksiin", "finnish": "Mitä kuuluu?", "chinese": "你好吗？" },
    { "finnish": "Hyvin, kiitos.", "chinese": "很好，谢谢。" },

    { "finnish": "Kuka sinä olet?", "chinese": "你是谁？" },
    { "finnish": "Minä olen Hao.", "chinese": "我是浩。" },

    { "finnish": "Mikä sinun etunimi on?", "chinese": "你的名字是什么？" },
    { "finnish": "Minun etunimi on Hao.", "chinese": "我的名字叫浩。" },

    { "finnish": "Mikä sinun sukunimi on?", "chinese": "你的姓是什么？" },
    { "finnish": "Minun sukunimi on Zhang.", "chinese": "我的姓是Zhang。" },

    { "finnish": "Missä sinä asut?", "chinese": "你住在哪里？" },
    { "finnish": "Minä asun Turussa.", "chinese": "我住在图尔库。" },

    { "finnish": "Minkä maalainen sinä olet?", "chinese": "你是哪国人？" },
